rules_version = '2';

service cloud.firestore {
  match /databases/jh-interstellar-clash/documents {
  	// read-only collection
    match /people/{person} {
      allow get;
      allow list;
      allow create: if false;
      allow update: if false;
      allow delete: if false;
    }
    // read-only collection
    match /starships/{starship} {
      allow get;
      allow list;
      allow create: if false;
      allow update: if false;
      allow delete: if false;
    }
  }
}
